<% if notice %>
  <div id="notice" class="alert alert-success text-center" role="alert"><%= notice %></div>
<% end %>

<div class="page-header">

  <div class = "col-md-1 pull-right">
    <%= link_to 'New Project', new_project_path, class: "btn btn-info btn-md" %>
  </div>
  <h1>Projects</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= link_to "#{project.name}", project_path(project) %> </td>
      </tr>
    <% end %>
  </tbody>
</table>
